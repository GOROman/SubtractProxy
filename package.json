{
  "name": "subtract-proxy",
  "version": "0.0.1",
  "description": "シンプルで軽量なWebプロキシサーバー - LLMによる情報フィルタリング機能付き",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm run format:check && npm run type-check && npm run test:coverage"
  },
  "author": "GOROman",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.11.28",
    "express": "^4.18.3",
    "http-proxy": "^1.18.1",
    "ollama": "^0.4.9",
    "openai": "^4.88.0",
    "typescript": "^5.4.2",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/http-proxy": "^1.17.14",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  }
}
